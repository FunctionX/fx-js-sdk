    "eslint-plugin-simple-import-sort": "^7.0.0",
    "glob": "^7.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^8",
    "ts-proto": "^1.92.1",
