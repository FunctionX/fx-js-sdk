{
  "name": "@functionx_io/fx-js-sdk",
  "version": "0.3.0",
  "description": "fxcore js sdk",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "dist/",
    "build/",
    "README.md",
    "package.json",
    "!*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/functionx/fx-js-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "zakir",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint --max-warnings 0 \"./**/*.ts\" \"./*.js\"",
    "lint-fix": "eslint --fix --max-warnings 0 \"./**/*.ts\" \"./*.js\"",
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "codegen": "./gen-proto.sh && yarn format",
    "build": "rm -rf ./build && tsc",
    "test": "npx jest --config jest.config.js --testPathIgnorePatterns=fxdex",
    "build-or-skip": "[ -n \"$SKIP_BUILD\" ] || yarn build",
    "pack-web": "yarn build-or-skip && webpack --mode development --config webpack.web.config.js"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.30.1",
    "@cosmjs/crypto": "^0.30.1",
    "@cosmjs/encoding": "^0.30.1",
    "@cosmjs/proto-signing": "^0.30.1",
    "@cosmjs/stargate": "^0.30.1",
    "cosmjs-types": "^0.7.1",
    "ethers": "^5.6.4",
    "long": "^4.0.0",
    "protobufjs": "^6.11.3"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/long": "^4.0.1",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.145.0",
    "typescript": "^4.9.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.6.0"
  }
}
